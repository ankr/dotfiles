[alias]
	st = status
	co = checkout
	nb = checkout -b
	ap = add -p
	df = diff
	ci = commit
	cv = commit -v
	br = branch
	ca = commit --amend
	cp = cherry-pick
	lg = log -p
	fakeignore = update-index --assume-unchanged
[color]
	branch  = auto
	diff = auto
	status = auto
[push]
	default = current
[include]
	path = ~/.dotfiles/git_custom
